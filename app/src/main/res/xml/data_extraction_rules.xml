<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude DataStore preferences from cloud backup -->
        <exclude domain="sharedpref" path="datastore"/>
    </cloud-backup>
    <device-transfer>
        <exclude domain="sharedpref" path="datastore"/>
    </device-transfer>
</data-extraction-rules>
